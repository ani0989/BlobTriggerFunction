{
  "bindings": [
    {
      "type": "eventHub",
      "name": "$return",
      "eventHubName": "%eventHubName%",
      "connection": "MyEventHubSendAppSetting",
      "direction": "out"
    },
    {
      "type": "blobTrigger",
      "direction": "in",
      "name": "myblob",
      "path": "%pathName%",
      "connection": "AzureWebJobsStorage"
    }
  ]
}